<table class="table">
    <thead>
      <tr>
        <th>Customer</th>
        <th>Rating</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
      <% review.each do |i| %>
      <tr>
        <td><%=current_user.first_name%></td>
        <td><%=i.rating %>/5</td>
        <td><%=i.description %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
